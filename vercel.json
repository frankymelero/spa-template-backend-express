{
    "version": 2,
    "name": "socketing",
    "builds": [
      {
        "src": "src/routes/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/v1/(.*)", 
        "dest": "src/routes/index.js"
      },
      {
        "src": "/(.*)", 
        "dest": "src/routes/index.js"
      }
    ]
  }